{
  "$schema": "https://v2-8-8.turborepo.dev/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "build:docker": {},
    "format": {},
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "outputs": [
        "coverage/**"
      ]
    },
    "test:e2e": {
      "outputs": [
        "coverage/**"
      ]
    }
  }
}